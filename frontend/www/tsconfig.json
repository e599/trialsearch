{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "inlineSources": true,
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "types": ["node", "googlemaps", "cypress"],
    "downlevelIteration": true
  },
  "exclude": ["out", ".next"]
}
